[submodule "src/git"]
	path = src/git
	url = https://github.com/git/git.git
	branch = maint
[submodule "src/muen"]
	path = src/muen/muen
	url = ../muen.git
	branch = devel
[submodule "src/zlib"]
	path = src/zlib
	url = https://github.com/madler/zlib.git
	ignore = dirty
[submodule "src/u-root/src/github.com/u-root/u-root"]
	path = src/u-root/src/github.com/u-root/u-root
	url = https://github.com/u-root/u-root.git
[submodule "src/e2fsprogs"]
	path = src/e2fsprogs
	url = https://git.kernel.org/pub/scm/fs/ext2/e2fsprogs.git
	branch = maint
[submodule "src/coreboot/coreboot"]
	path = src/coreboot/coreboot
	url = ../coreboot.git
	branch = nitrohsm
[submodule "src/coreboot/grub2"]
	path = src/coreboot/grub2
	url = ../grub.git
	branch = nitrohsm
[submodule "src/flashrom"]
	path = src/flashrom
	url = ../flashrom.git
	branch = nethsm
[submodule "src/pciutils"]
	path = src/pciutils
	url = https://github.com/pciutils/pciutils
	ignore = dirty
