[submodule "src/muen"]
	path = src/muen/muen
	url = ../muen.git
	branch = devel
[submodule "src/u-root/src/github.com/u-root/u-root"]
	path = src/u-root/src/github.com/u-root/u-root
	url = https://github.com/u-root/u-root.git
[submodule "src/e2fsprogs"]
	path = src/e2fsprogs
	url = https://github.com/tytso/e2fsprogs.git
	branch = maint
[submodule "src/coreboot/coreboot"]
	path = src/coreboot/coreboot
	url = ../coreboot.git
	branch = nethsm
	ignore = dirty
[submodule "src/coreboot/grub2"]
	path = src/coreboot/grub2
	url = ../grub.git
	branch = nethsm
[submodule "src/u-root/src/github.com/google/go-tpm"]
	path = src/u-root/src/github.com/google/go-tpm
	url = https://github.com/google/go-tpm
	branch = master
[submodule "src/u-root/src/golang.org/x/sys"]
	path = src/u-root/src/golang.org/x/sys
	url = https://github.com/golang/sys
	branch = master
[submodule "src/util-linux"]
	path = src/util-linux
	url = ../util-linux.git
	branch = nethsm
